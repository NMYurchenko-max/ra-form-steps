  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint \"src/**/*.ts?(x|js)\" --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint \"src/**/*.ts?(x|js)\" --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier \"src/**/*.ts?(x|js)\" --write",
    "preview": "vite preview",
    "deploy": "gh-pages -d dist",
    "typecheck": "tsc --noEmit",
    "test": "cypress run",
    "test:open": "cypress open",
    "test:e2e": "cypress run --spec \"cypress/e2e/app.cy.ts\"",
    "test:wait": "timeout /t 5 && yarn test:e2e"
  },
  "dependencies": {
    "@types/uuid": "^10.0.0",
    "classnames": "2.5.1",
    "emoji-picker-react": "4.13.2",
